    "karma-nested-reporter": "0.1.3",
    "karma-opera-launcher": "0.3.0",
    "karma-phantomjs-launcher": "0.2.1",
    "karma-safari-launcher": "0.1.1",
    "karma-sauce-launcher": "0.2.14",
